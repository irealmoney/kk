    <div class="form-alignment">
 
        <form class="admin-form" method="POST" action="/auth/register" novalidate>



            <img src="/images/recommender-03.png" width="84px" height="84px" alt="">
            <h2>ساخت حساب کاربری</h2>

                <div class="input-flex-r">
                    <div class="MD-login-input">

                        <input type="hidden" name="_csrf" value="<%= req.csrfToken() %>">
                        
                        <p><label for="name">نام و نام خانوادگی  </label></p>
                        <input type="text" id="name" name="name" value="<%= old('name') %>">


                        <p><label for="username">نام کاربری  </label></p>
                        <input type="text" id="username" name="username" value="<%= old('username') %>">


                        <p><label for="phone">شماره تماس </label></p>
                        <input type="tel" id="phone" name="phone" value="<%= old('phon') %>">

                    </div>


                    <div class="MD-login-input">

                        <p><label for="email">ایمیل </label></p>
                        <input type="email" id="email" name="email" value="<%= old('email') %>">


                        <p><label for="password">رمز عبور </label></p>
                        <input type="password" id="password" name="password" >

                        <div style="margin-top: 10px;">
                        <%- recaptcha %>
                        </div>
                    </div>
                </div>        

            <div class="login-btn">
                <a href="/">بازگشت</a>
                <input type="submit" id="submitbtn" name="login-btn"  value="ثبت">
            </div>
        </form>
    </div>






